<% include ./partials/header %>

<div class='container'>
    <div class='jumbotron'>
        <form action='/insertsalesman' method='POST'>
            <div class='form-group'>
                <label> Salesman ID </label>
                <input type='text' name='Salesman[salesman_id]' class='form-control' placeholder='Salesman ID'>
            </div>
            <div class='form-group'>
                <label>First Name</label>
                <input type='text' name='Salesman[first_name]' class='form-control' placeholder='First Name'>
            </div>
            <div class='form-group'>
                <label>Last Name</label>
                <input type='text' name='Salesman[last_name]' class='form-control' placeholder='Last Name'>
            </div>
            <div class='form-group'>
                <label>SSN</label>
                <input type='password' name='Salesman[ssn]' class='form-control' placeholder='SSN'>
            </div>
            <button type='submit' class='btn btn-primary center-block'>Submit</button>
        </form>
    </div>
</div>


<% include ./partials/footer %>
